                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.CreateSightingRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/errorhandler.Error"
                        }
                    },
                    "409": {
                        "description": "Conflict",
